<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="vn.asiantech.internship">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="vn.asiantech.internship.ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".savedata.SharePreferenceActivity" />
        <activity android:name=".savedata.ExternalStorageActivity" />
        <activity android:name=".savedata.SaveDatabaseActivity" />
        <activity android:name=".savedata.CompanyActivity"/>
        <activity android:name=".ui.calculator.CalculatorActivity"/>
        <activity android:name=".ui.recyclerview.PersonViewActivity"/>
        <activity android:name=".ui.login.LoginActivity"/>
        <activity android:name=".savedata.SaveDataActivity"/>
    </application>
</manifest>
