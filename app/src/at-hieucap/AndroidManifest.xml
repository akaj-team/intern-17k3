<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.asiantech.internship">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.viewpager.home.InitialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.viewpager.information.HomeActivity" />
        <activity android:name=".ui.calculator.CalculatorActivity" />
        <activity android:name=".ui.recyclerview.PersonViewActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.loadimage.MainActivity" />
        <activity android:name=".ui.loadimage.LoadImageActivity" />
        <activity android:name=".ui.loadimage.ImageVectorActivity" />
        <activity android:name=".ui.loadimage.NinePathActivity" />
    </application>
</manifest>
