<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Set màu nền-->
    <solid
        android:color="#ffff" >
    </solid>

    <!-- đặt thuộc tính màu và độ rộng của đường viền -->
    <stroke
        android:width="2dp"
        android:color="#0101DF" >
    </stroke>

    <!-- bán kính đường tròn ở 4 góc -->
    <corners
        android:radius="10dp" >
    </corners>

</shape>